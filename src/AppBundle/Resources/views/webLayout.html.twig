<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>RESERVING - {% block title %}{% endblock %}</title>

    {% block stylesheets %}{% endblock %}
    <link rel="stylesheet" href="{{ asset("bower_components/bootstrap/dist/css/bootstrap.min.css") }}"/>
    <link rel="stylesheet" type="text/css" href="{{ asset('web_assets/css/main.css') }}" />
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body id="{% if currentpage is defined %}{{ currentpage }}{% endif %}">
{% block body %}
<div class="page">
    {% block header%}
        <!-- .page-header-->
        <header class="page-header">
            {% include '@App/web/shared/web_header.html.twig' %}
        </header>
    {% endblock%}

    <!-- .page-content -->
    <div class="page-content">
        {% block content %}{% endblock %}
    </div>

    {% block footer%}
        <!-- .page-footer -->
        <footer class="page-footer">
            {% include "@App/web/shared/web_footer.html.twig" %}
        </footer>
    {% endblock %}
</div>
{% endblock %}
    <script src="{{ asset('bower_components/angular/angular.js') }}"></script>
    <script src="{{ asset('bower_components/angular-ui-router/release/angular-ui-router.js') }}"></script>
    <script src="{{ asset('web_assets/js/main.js') }}"></script>
{% block javascripts %}{% endblock %}
</body>
</html>

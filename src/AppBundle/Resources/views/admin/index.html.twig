<!doctype html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <title>Reserving - WebApp</title>
</head>
<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.min.js"></script>
<script>
    angular.module('app', [])
            .config(function($interpolateProvider) {
                $interpolateProvider.startSymbol('[[');
                $interpolateProvider.endSymbol(']]');
            })

            .controller('appCtrl', ['$scope', function($scope){
                $scope.empleados = [{name: "Geordano"},{name:"Beatriz"}];
            }])
</script>
<body ng-app="app">

<div class="" ng-controller="appCtrl">
    <ul>
        <input type="text" ng-model="texto"/> <strong>[[ texto ]]</strong>
        <li ng-repeat="empleado in empleados">[[ empleado.name | filter:text]]</li>
    </ul>
</div>
<!-- Scripts -->
</body>
</html>
<!DOCTYPE html>
<html ng-app="reserving">
<head>
    <meta charset="utf-8">
    <title>Reserving | Gestión de citas online</title>
    <meta name="description" content="Reserving, el gestor de citas que te ayudará a organizar tu negocio.">
    <meta name="viewport" content="width=device-width">
    <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->

    {% block stylesheets %}
        <!-- build:css(.) styles/vendor.css -->
        <!-- bower:css -->
        <link rel="stylesheet" href="{{ asset("bower_components/bootstrap/dist/css/bootstrap.css") }}" />
        <link rel="stylesheet" href="{{ asset("bower_components/font-awesome/css/font-awesome.css") }}" />
        <link rel="stylesheet" href="{{ asset("bower_components/angular-loading-bar/build/loading-bar.css") }}" />
        <link rel="stylesheet" href="{{ asset("bower_components/metisMenu/dist/metisMenu.css") }}" />
        <!-- endbower -->
        <!-- endbuild -->
        <!-- build:css(.tmp) styles/main.css -->
        <link rel="stylesheet" href="{{ asset("app/css/main.css") }}">
        <!-- endbuild -->
    {% endblock %}
    <link rel="stylesheet" type="text/css" href="{{ asset('web_assets/css/main.css') }}" />
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>

<body id="{% if currentpage is defined %}{{ currentpage }}{% endif %}">
{% block body %}
    {% include '@App/admin/shared/admin_header.html.twig' %}

    {% block content %}{% endblock %}

    {% include "@App/admin/shared/admin_footer.html.twig" %}
{% endblock %}
{% block javascripts %}
    <!-- build:js(.) scripts/vendor.js -->
    <!-- bower:js -->
    <script src="{{ asset("bower_components/jquery/dist/jquery.js") }}"></script>
    <script src="{{ asset("bower_components/angular/angular.js") }}"></script>
    <script src="{{ asset("bower_components/bootstrap/dist/js/bootstrap.js") }}"></script>
    <script src="{{ asset("bower_components/angular-animate/angular-animate.js") }}"></script>
    <script src="{{ asset("bower_components/angular-cookies/angular-cookies.js") }}"></script>
    <script src="{{ asset("bower_components/angular-resource/angular-resource.js") }}"></script>
    <script src="{{ asset("bower_components/angular-touch/angular-touch.js") }}"></script>
    <script src="{{ asset("bower_components/angular-loading-bar/build/loading-bar.js") }}"></script>
    <script src="{{ asset("bower_components/angular-bootstrap/ui-bootstrap-tpls.js") }}"></script>
    <script src="{{ asset("bower_components/angular-ui-router/release/angular-ui-router.js") }}"></script>
    <script src="{{ asset("bower_components/metisMenu/dist/metisMenu.js") }}"></script>
    <!-- endbower -->
    <!-- endbuild -->

    <!-- build:js({.tmp,app}) scripts/scripts.js -->
    <script src="{{ asset("app/scripts/app.js") }}"></script>
    <script src="{{ asset("app/scripts/tools.js") }}"></script>
    <script src="{{ asset("app/scripts/controllers.js") }}"></script>
    <script src="{{ asset("app/scripts/directives.js") }}"></script>
    <script src="{{ asset("app/scripts/filters.js") }}"></script>
    <script src="{{ asset("app/scripts/services.js") }}"></script>
    <script src="{{ asset("app/js/reserving.js") }}"></script>
    <!-- endbuild -->

{% endblock %}
</body>
</html>
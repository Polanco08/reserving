<!DOCTYPE html>
<html ng-app="reservingApp">
<head>
    <meta charset="utf-8">
    <title>Reserving | {% block title %}{% endblock %}</title>
    <meta name="description" content="Reserving, el gestor de citas que te ayudarÃ¡ a organizar tu negocio.">
    <meta name="viewport" content="width=device-width">
    <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
    <link rel="apple-touch-icon" sizes="180x180" href="{{asset("apple-icon-180x180.png")}}">
    <link rel="icon" type="image/png" sizes="192x192"  href="{{asset("android-icon-192x192.png")}}">
    <link rel="icon" type="image/png" sizes="32x32" href="{{asset("favicon-32x32.png")}}">



    {% block stylesheets %}
        <!-- build:css(.) styles/vendor.css -->
        <!-- bower:css -->
        <link rel="stylesheet" href="{{ asset("assets/web/css/bootstrap_theme.css") }}" />
        <link rel="stylesheet" href="{{ asset("bower_components/font-awesome/css/font-awesome.css") }}" />
        <link rel="stylesheet" href="{{ asset("bower_components/angular-loading-bar/build/loading-bar.css") }}" />
        <link rel="stylesheet" href="{{ asset("bower_components/sweetalert/dist/sweetalert.css") }}" />
        <!-- endbower -->
        <!-- endbuild -->
        <!-- build:css(.tmp) styles/main.css -->
        <link rel="stylesheet" href="{{ asset("assets/app/css/main.css") }}">
        <!-- endbuild -->
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="" />
</head>

<body id="{% if currentpage is defined %}{{ currentpage }}{% endif %}" class="">
    <section id="page" class="main-page">
        {% block body %}
            {% block content %}{% endblock %}
        {% endblock %}
    </section>
{% block javascripts %}
    <!-- build:js(.) scripts/vendor.js -->
    <!-- bower:js -->
    <script src="{{ asset("bower_components/jquery/dist/jquery.js") }}"></script>
    <script src="{{ asset("bower_components/angular/angular.js") }}"></script>
    <script src="{{ asset("bower_components/bootstrap/dist/js/bootstrap.js") }}"></script>
    <script src="{{ asset("bower_components/angular-animate/angular-animate.js") }}"></script>
    <script src="{{ asset("bower_components/angular-cookies/angular-cookies.js") }}"></script>
    <script src="{{ asset("bower_components/angular-resource/angular-resource.js") }}"></script>
    <script src="{{ asset("bower_components/angular-touch/angular-touch.js") }}"></script>
    <script src="{{ asset("bower_components/angular-loading-bar/build/loading-bar.js") }}"></script>
    <script src="{{ asset("bower_components/angular-bootstrap/ui-bootstrap-tpls.js") }}"></script>
    <script src="{{ asset("bower_components/angular-ui-router/release/angular-ui-router.js") }}"></script>
    <script src="{{ asset("bower_components/angular-breadcrumb/dist/angular-breadcrumb.js") }}"></script>
    <script src="{{ asset("bower_components/sweetalert/dist/sweetalert.min.js") }}"></script>
    <script src="{{ asset("bower_components/xtform/xtForm.js") }}"></script>
    <script src="{{ asset("bower_components/angular-dropdown-multiselect/dist/angular-dropdown-multiselect.js") }}"></script>

    <!-- endbower -->
    <!-- endbuild -->

    <!-- build:js({.tmp,app}) scripts/scripts.js -->
    <script src="{{ asset("app/tools.js") }}"></script>
    <script src="{{ asset("app/empleados/EmpleadosService.js") }}"></script>
    <script src="{{ asset("app/empleados/EmpleadosController.js") }}"></script>

    <script src="{{ asset("app/clientes/clientesService.js") }}"></script>
    <script src="{{ asset("app/clientes/clientesController.js") }}"></script>

    <script src="{{ asset("app/citas/CitasService.js") }}"></script>
    <script src="{{ asset("app/citas/CitasController.js") }}"></script>

    <script src="{{ asset("app/servicios/ServiciosService.js") }}"></script>
    <script src="{{ asset("app/servicios/ServiciosController.js") }}"></script>

    <script src="{{ asset("app/app.js") }}"></script>
    <script src="{{ asset("assets/app/js/main.js")}}"></script>

    <!-- endbuild -->

{% endblock %}
</body>
</html>
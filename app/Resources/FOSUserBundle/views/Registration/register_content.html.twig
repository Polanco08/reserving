{% trans_default_domain 'FOSUserBundle' %}

    <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }}
          method="POST" class="fos_user_registration_register">

        {{ form_widget(form) }}

        <button class="btn btn-success btn-block btn-lg" type="submit" id="_submit">
            <i class="glyphicon glyphicon-log-in"></i> {{ 'registration.submit'|trans }}
        </button>
    </form>

